{
  "name": "page-build",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run build:file && vue-cli-service serve",
    "build": "npm run build:file && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --mode=development",
    "build:file": "node ./script/build-file.js",
    "build:img": "npm run build:file && ts-node -r tsconfig-paths/register ./script/build-imgs.ts --componentName ",
    "build:admin": "cross-env BUILD_TARGET=admin vue-cli-service build --report",
    "build:web": "cross-env BUILD_TARGET=web vue-cli-service build --report",
    "build:components": "cross-env BUILD_TARGET=components vue-cli-service build --entry=src/components/index.ts --dest=dist/components --name=rp-components --target=lib  --report"
  },
  "dependencies": {
    "@types/html2canvas": "0.0.36",
    "animate.css": "^4.1.1",
    "axios": "^0.21.0",
    "core-js": "^3.6.5",
    "debounce-throttle": "^1.0.4",
    "echarts": "^4.9.0",
    "element-ui": "^2.14.0",
    "html2canvas": "^1.0.0-rc.7",
    "normalize.css": "^8.0.1",
    "pinyin": "^2.9.1",
    "qs": "^6.9.4",
    "swiper": "^6.3.5",
    "tyshemo": "^9.10.3",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-codemirror": "^4.0.6",
    "vue-draggable-resizable-gorkys": "^2.4.4",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vue-touch-keyboard": "^0.3.2",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2",
    "vuex-module-decorators": "^1.0.1",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@types/echarts": "^4.9.0",
    "@types/jest": "^24.0.19",
    "@types/node": "^14.14.9",
    "@types/puppeteer": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "puppeteer": "^5.5.0",
    "px2rem-loader": "^0.1.9",
    "sass-loader": "^8.0.2",
    "style-resources-loader": "^1.3.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "~3.9.3",
    "vue-template-compiler": "^2.6.11"
  }
}
